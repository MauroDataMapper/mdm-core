import uk.ac.ox.softeng.maurodatamapper.core.rest.converter.json.OffsetDateTimeConverter
import uk.ac.ox.softeng.maurodatamapper.datamodel.facet.summarymetadata.SummaryMetadataReport

model {
    SummaryMetadataReport export
}

json {
    if (export.id) id export.id
    if (export.lastUpdated) lastUpdated OffsetDateTimeConverter.toString(export.lastUpdated)
    reportDate OffsetDateTimeConverter.toString(export.reportDate)
    reportValue export.reportValue
}
